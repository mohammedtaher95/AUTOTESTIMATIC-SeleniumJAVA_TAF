<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="WebDriver Suite" guice-stage="DEVELOPMENT">
  <parameter name="CROSS_BROWSER_MODE" value="SEQUENTIAL"/>
  <parameter name="ENV_TYPE" value="LOCAL"/>
  <parameter name="REMOTE_ENV_URL" value="http://localhost:4444/wd/hub/"/>
  <test thread-count="1" name="Chrome Test">
    <parameter name="browserName" value="chrome"/>
    <classes>
      <class name="tests.TestClass"/>
    </classes>
  </test> <!-- Chrome Test -->
  <test thread-count="1" name="Firefox Test">
    <parameter name="browserName" value="firefox"/>
    <classes>
      <class name="tests.TestClass"/>
    </classes>
  </test> <!-- Firefox Test -->
</suite> <!-- WebDriver Suite -->
